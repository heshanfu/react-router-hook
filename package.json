{
  "name": "react-router-hook",
  "version": "0.2.0",
  "description": "Universal data fetching and lifecycle management for react router with multiple components",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-router-hook.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/react-router-hook.min.js --config webpack.config.production.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/react-router-hook.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "react",
    "react-router",
    "hook",
    "async",
    "fetch",
    "react-component",
    "reactjs"
  ],
  "author": [
    "Bin Hou <houbin217jz@gmail.com> (https://twitter.com/houbin217jz)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "estraverse-fb": "^1.3.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "async": "^2.0.1",
    "debug": "^2.2.0",
    "react-addons-shallow-compare": "^15.3.2",
    "react-addons-update": "^15.3.2"
  }
}
